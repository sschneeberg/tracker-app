<main class="userhome">
    <h2>Profile Page</h2>
    <% periods.forEach(period => { %>
        <p>period start:
            <%= period.startDate %>
        </p>
        <p>period end:
            <%= period.endDate %>
        </p>
        <% }) %>
            <% periodWeeks.forEach(week => { %>
                <p>week:
                    <%= week %>
                </p>
                <% }) %>
                    <% notes.forEach(notes => { %>
                        <% notes.forEach(note => { %>
                            <p>note:
                                <%= note.date %>
                                    <%= note.title %>
                                        <%= note.content %>
                            </p>
                            <% }) %>
                                <% }) %>
                                    <% symptoms.forEach(symptoms => { %>
                                        <%symptoms.forEach(symptom => { %>
                                            <p>
                                                symptom:
                                                <%= symptom.date %>,
                                                    <%= symptom.type %>,
                                                        <%= symptom.severity %>
                                            </p>
                                            <% }) %>
                                                <% }) %>

                                                    <div class="calendar">
                                                        <% monthData.forEach(day => { %>
                                                            <a href="">
                                                                <div class="day">
                                                                    <p>
                                                                        <%= day.notes %>
                                                                    </p>
                                                                    <p>
                                                                        <%= day.symptoms %>
                                                                    </p>
                                                                    <p>
                                                                        <%= day.period %>
                                                                    </p>
                                                                </div>
                                                            </a>
                                                            <% }) %>
                                                    </div>
</main>